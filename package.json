    "vite-plugin-svgr": "^2.4.0",
    "vite-tsconfig-paths": "^4.0.5",
    "vitest": "^0.28.4",
    "yarn": ">=1.22.19"
  },
  "scripts": {
    "postinstall": "patch-package",
    "start": "vite",
    "build": "tsc && vite build",
    "serve": "vite preview",
    "start:developBackend": "GENERATE_SOURCEMAP=false bash -ac '. .env.development.developBackend; vite'",
    "start-env": "bash -ac '. .env.${VITE_ENV}; vite'",
    "build-env": "bash -ac '. .env.${VITE_ENV}; tsc && vite build'",
    "build:lifinance:staging": "VITE_ENV=lifinance-mainnet.staging npm run build-env",
    "build:lifinance:develop": "VITE_ENV=lifinance-mainnet.develop npm run build-env",
    "build:lifinance": "VITE_ENV=lifinance-mainnet npm run build-env",
    "pre-commit": "lint-staged",
    "lint:fix": "eslint --ext .tsx --ext .ts ./src --fix",
    "prettier:fix": "prettier --write ./src/.",
    "sdk:link": "yarn link '@lifi/sdk'",
    "sdk:unlink": "yarn unlink '@lifi/sdk' && yarn sdk:update",
    "sdk:update": "yarn install --force",
    "prepare": "husky install",
    "winBuild": "set 'GENERATE_SOURCEMAP=false' && bash -ac '. .env.development.developBackend; vite'",
    "test": "vitest",
    "pre-push": "vitest run"
  },
  "lint-staged": {
    "src/**/*.{ts,tsx}": [
      "yarn run lint:fix",
      "yarn run prettier:fix"
    ]
  },
  "browserslist": [
    ">0.2%",
    "not dead",
    "not op_mini all"
  ]
}
